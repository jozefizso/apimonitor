<ApiMonitor>
    <Include Filename="Headers\windows.h.xml" />
    <Include Filename="Headers\cryptography.h.xml" />
    <Headers>

        <!-- TODO: Unions (Complex) -->
        <Variable Name="PSEC_WINNT_AUTH_IDENTITY_OPAQUE"    Type="Alias"    Base="LPVOID" />
        <Variable Name="PSEC_WINNT_AUTH_IDENTITY_OPAQUE*"   Type="Pointer"  Base="PSEC_WINNT_AUTH_IDENTITY_OPAQUE" />

        <!-- Variables -->
        <!-- TODO: SECURITY_STATUS - Overwrite S_OK with SEC_E_OK? -->
        <Variable Name="SECURITY_STATUS"                Type="Alias"    Base="HRESULT" />
        <Variable Name="SAFER_LEVEL_HANDLE"             Type="Alias"    Base="HANDLE" />
        <Variable Name="SAFER_LEVEL_HANDLE*"            Type="Pointer"  Base="SAFER_LEVEL_HANDLE" />
        <Variable Name="BYTE [SAFER_MAX_HASH_SIZE]"     Type="Array"    Base="BYTE" Count="64" />
        <Variable Name="CHAR [TOKEN_SOURCE_LENGTH]"     Type="Array"    Base="CHAR" Count="8" />

        <!-- TODO: External Interfaces -->
        <Variable Name="ISecurityInformation"   Type="Interface" />

        <!-- Interface Aliases -->
        <Variable Name="LPSECURITYINFO"         Type="Alias"    Base="ISecurityInformation*" />
        <Variable Name="LPSECURITYINFO*"        Type="Pointer"  Base="LPSECURITYINFO" />

        <!-- [AceFlags] -->
        <Variable Name="[AceFlags]" Type="Alias" Base="DWORD">
            <Display Name="DWORD" />
            <Flag>
                <Set Name="OBJECT_INHERIT_ACE"                  Value="0x1" />
                <Set Name="CONTAINER_INHERIT_ACE"               Value="0x2" />
                <Set Name="SUB_CONTAINERS_AND_OBJECTS_INHERIT"  Value="0x3" />
                <Set Name="NO_PROPAGATE_INHERIT_ACE"            Value="0x4" />
                <Set Name="INHERIT_ONLY_ACE"                    Value="0x8" />
                <Set Name="INHERITED_ACE"                       Value="0x10" />
            </Flag>
        </Variable>

        <!-- ACCESS_MODE -->
        <Variable Name="ACCESS_MODE" Type="Alias" Base="UINT">
            <Enum>
                <Set Name="NOT_USED_ACCESS"     Value="0" />
                <Set Name="GRANT_ACCESS"        Value="1" />
                <Set Name="SET_ACCESS"          Value="2" />
                <Set Name="DENY_ACCESS"         Value="3" />
                <Set Name="REVOKE_ACCESS"       Value="4" />
                <Set Name="SET_AUDIT_SUCCESS"   Value="5" />
                <Set Name="SET_AUDIT_FAILURE"   Value="6" />
            </Enum>
        </Variable>

        <!-- MULTIPLE_TRUSTEE_OPERATION -->
        <Variable Name="MULTIPLE_TRUSTEE_OPERATION" Type="Alias" Base="UINT">
            <Enum>
                <Set Name="NO_MULTIPLE_TRUSTEE"     Value="0" />
                <Set Name="TRUSTEE_IS_IMPERSONATE"  Value="1" />
            </Enum>
        </Variable>

        <!-- TRUSTEE_FORM -->
        <Variable Name="TRUSTEE_FORM" Type="Alias" Base="UINT">
            <Enum>
                <Set Name="TRUSTEE_IS_SID"              Value="0" />
                <Set Name="TRUSTEE_IS_NAME"             Value="1" />
                <Set Name="TRUSTEE_BAD_FORM"            Value="2" />
                <Set Name="TRUSTEE_IS_OBJECTS_AND_SID"  Value="3" />
                <Set Name="TRUSTEE_IS_OBJECTS_AND_NAME" Value="4" />
            </Enum>
        </Variable>

        <!-- TRUSTEE_TYPE -->
        <Variable Name="TRUSTEE_TYPE" Type="Alias" Base="UINT">
            <Enum>
                <Set Name="TRUSTEE_IS_UNKNOWN"          Value="0" />
                <Set Name="TRUSTEE_IS_USER"             Value="1" />
                <Set Name="TRUSTEE_IS_GROUP"            Value="2" />
                <Set Name="TRUSTEE_IS_DOMAIN"           Value="3" />
                <Set Name="TRUSTEE_IS_ALIAS"            Value="4" />
                <Set Name="TRUSTEE_IS_WELL_KNOWN_GROUP" Value="5" />
                <Set Name="TRUSTEE_IS_DELETED"          Value="6" />
                <Set Name="TRUSTEE_IS_INVALID"          Value="7" />
                <Set Name="TRUSTEE_IS_COMPUTER"         Value="8" />
            </Enum>
        </Variable>

        <!-- OBJECTS_AND_SID -->
        <Variable Name="OBJECTS_AND_SID" Type="Struct">
            <Field Type="DWORD"   	Name="ObjectsPresent" />
            <Field Type="GUID"    	Name="ObjectTypeGuid" />
            <Field Type="GUID"    	Name="InheritedObjectTypeGuid" />
            <Field Type="SID*"      Name="pSid" />
        </Variable>
        <Variable Name="POBJECTS_AND_SID" Type="Pointer" Base="OBJECTS_AND_SID" />
        <Variable Name="OBJECTS_AND_SID*" Type="Pointer" Base="OBJECTS_AND_SID" />

        <!-- SE_OBJECT_TYPE -->
        <Variable Name="SE_OBJECT_TYPE" Type="Alias" Base="UINT">
            <Enum>
                <Set Name="SE_UNKNOWN_OBJECT_TYPE"      Value="0" />
                <Set Name="SE_FILE_OBJECT"              Value="1" />
                <Set Name="SE_SERVICE"                  Value="2" />
                <Set Name="SE_PRINTER"                  Value="3" />
                <Set Name="SE_REGISTRY_KEY"             Value="4" />
                <Set Name="SE_LMSHARE"                  Value="5" />
                <Set Name="SE_KERNEL_OBJECT"            Value="6" />
                <Set Name="SE_WINDOW_OBJECT"            Value="7" />
                <Set Name="SE_DS_OBJECT"                Value="8" />
                <Set Name="SE_DS_OBJECT_ALL"            Value="9" />
                <Set Name="SE_PROVIDER_DEFINED_OBJECT"  Value="10" />
                <Set Name="SE_WMIGUID_OBJECT"           Value="11" />
                <Set Name="SE_REGISTRY_WOW64_32KEY"     Value="12" />
            </Enum>
        </Variable>

        <!-- OBJECTS_AND_NAME -->
        <Variable Name="OBJECTS_AND_NAME" Type="Struct">
            <Field Type="DWORD"          	Name="ObjectsPresent" />
            <Field Type="SE_OBJECT_TYPE" 	Name="ObjectType" />
            <Field Type="LPTSTR"   	        Name="ObjectTypeName" />
            <Field Type="LPTSTR"   	        Name="InheritedObjectTypeName" />
            <Field Type="LPTSTR"   	        Name="ptstrName" />
        </Variable>
        <Variable Name="POBJECTS_AND_NAME" Type="Pointer" Base="OBJECTS_AND_NAME" />
        <Variable Name="OBJECTS_AND_NAME*" Type="Pointer" Base="OBJECTS_AND_NAME" />

        <!-- [TRUSTEE_u] -->
        <Variable Name="[TRUSTEE_u]" Type="Union">
            <Display Name="union" />
            <Field Type="LPTSTR"                Name="ptstrName" />
            <Field Type="SID*"                  Name="pSid" />
            <Field Type="OBJECTS_AND_SID*"      Name="pObjectsAndSid" />
            <Field Type="OBJECTS_AND_NAME*"     Name="pObjectsAndName" />
        </Variable>

        <!-- TRUSTEE -->
        <Variable Name="TRUSTEE" Type="Struct">
            <!-- TODO: Self-Referencing - pMultipleTrustee is TRUSTEE* -->
            <Field Type="LPVOID"                        Name="pMultipleTrustee" Display="TRUSTEE*" />
            <Field Type="MULTIPLE_TRUSTEE_OPERATION"    Name="MultipleTrusteeOperation" />
            <Field Type="TRUSTEE_FORM"                  Name="TrusteeForm" />
            <Field Type="TRUSTEE_TYPE"                  Name="TrusteeType" />
            <Field Type="[TRUSTEE_u]"                   Name="" />
        </Variable>
        <Variable Name="PTRUSTEE"   Type="Pointer" Base="TRUSTEE" />
        <Variable Name="PTRUSTEE*"  Type="Pointer" Base="PTRUSTEE" />

        <!-- EXPLICIT_ACCESS -->
        <Variable Name="EXPLICIT_ACCESS" Type="Struct">
            <Field Type="[ACCESS_MASK_DWORD]"   Name="grfAccessPermissions" />
            <Field Type="ACCESS_MODE"           Name="grfAccessMode" />
            <Field Type="[AceFlags]"            Name="grfInheritance" />
            <Field Type="TRUSTEE"               Name="Trustee" />
        </Variable>
        <Variable Name="PEXPLICIT_ACCESS"   Type="Pointer" Base="EXPLICIT_ACCESS" />
        <Variable Name="PEXPLICIT_ACCESS*"  Type="Pointer" Base="PEXPLICIT_ACCESS" />

        <!-- TOKEN_SOURCE -->
        <Variable Name="TOKEN_SOURCE" Type="Struct">
            <Field Type="CHAR [TOKEN_SOURCE_LENGTH]"    Name="SourceName" />
            <Field Type="LUID"                          Name="SourceIdentifier" />
        </Variable>
        <Variable Name="PTOKEN_SOURCE" Type="Pointer" Base="TOKEN_SOURCE" />

        <!-- [SAFER_CRITERIA] -->
        <Variable Name="[SAFER_CRITERIA]" Type="Alias" Base="DWORD">
            <Display Name="DWORD" />
            <Flag>
                <Set Name="SAFER_CRITERIA_IMAGEPATH"        Value="0x00001" />
                <Set Name="SAFER_CRITERIA_NOSIGNEDHASH"     Value="0x00002" />
                <Set Name="SAFER_CRITERIA_IMAGEHASH"        Value="0x00004" />
                <Set Name="SAFER_CRITERIA_AUTHENTICODE"     Value="0x00008" />
                <Set Name="SAFER_CRITERIA_URLZONE"          Value="0x00010" />
                <Set Name="SAFER_CRITERIA_IMAGEPATH_NT"     Value="0x01000" />
            </Flag>
        </Variable>

        <!-- SAFER_CODE_PROPERTIES -->
        <Variable Name="SAFER_CODE_PROPERTIES" Type="Struct" Pack="8">
            <Field Type="DWORD"                         Name="cbSize" />
            <Field Type="[SAFER_CRITERIA]"              Name="dwCheckFlags" />
            <Field Type="LPCWSTR"                       Name="ImagePath" />
            <Field Type="HANDLE"                        Name="hImageFileHandle" />
            <Field Type="URLZONE"                       Name="UrlZoneId" />
            <Field Type="BYTE [SAFER_MAX_HASH_SIZE]"    Name="ImageHash" />
            <Field Type="DWORD"                         Name="dwImageHashSize" />
            <Field Type="LARGE_INTEGER"                 Name="ImageSize" />
            <Field Type="ALG_ID"                        Name="HashAlgorithm" />
            <Field Type="LPBYTE"                        Name="pByteBlock" />
            <Field Type="HWND"                          Name="hWndParent" />
            <Field Type="[WTD_UI]"                      Name="dwWVTUIChoice" />
            <Field Type="LPCWSTR"                       Name="PackageMoniker" />
            <Field Type="LPCWSTR"                       Name="PackagePublisher" />
            <Field Type="LPCWSTR"                       Name="PackageName" />
            <Field Type="ULONG64"                       Name="PackageVersion" />
            <Field Type="BOOL"                          Name="PackageIsFramework" />
        </Variable>
        <Variable Name="PSAFER_CODE_PROPERTIES" Type="Pointer" Base="SAFER_CODE_PROPERTIES" />

        <!-- SID_IDENTIFIER_AUTHORITY -->
        <Variable Name="SID_IDENTIFIER_AUTHORITY" Type="Struct">
            <Field Type="BYTE [6]" Name="Value" />
        </Variable>
        <Variable Name="PSID_IDENTIFIER_AUTHORITY" Type="Pointer" Base="SID_IDENTIFIER_AUTHORITY" />

        <!-- AUDIT_POLICY_INFORMATION -->
        <Variable Name="AUDIT_POLICY_INFORMATION" Type="Struct">
            <Field Type="GUID"    	Name="AuditSubCategoryGuid" />
            <Field Type="ULONG"   	Name="AuditingInformation" />
            <Field Type="GUID"    	Name="AuditCategoryGuid" />
        </Variable>
        <Variable Name="PCAUDIT_POLICY_INFORMATION" Type="Pointer" Base="AUDIT_POLICY_INFORMATION" />
        <Variable Name="PAUDIT_POLICY_INFORMATION*" Type="Pointer" Base="PCAUDIT_POLICY_INFORMATION" />

        <!-- [AceType] -->
        <Variable Name="[AceType]" Type="Alias" Base="BYTE">
            <Display Name="BYTE" />
            <Enum>
                <Set Name="ACCESS_ALLOWED_ACE_TYPE"                     Value="0x0" />
                <Set Name="ACCESS_DENIED_ACE_TYPE"                      Value="0x1" />
                <Set Name="SYSTEM_AUDIT_ACE_TYPE"                       Value="0x2" />
                <Set Name="SYSTEM_ALARM_ACE_TYPE"                       Value="0x3" />
                <Set Name="ACCESS_ALLOWED_COMPOUND_ACE_TYPE"            Value="0x4" />
                <Set Name="ACCESS_ALLOWED_OBJECT_ACE_TYPE"              Value="0x5" />
                <Set Name="ACCESS_DENIED_OBJECT_ACE_TYPE"               Value="0x6" />
                <Set Name="SYSTEM_AUDIT_OBJECT_ACE_TYPE"                Value="0x7" />
                <Set Name="SYSTEM_ALARM_OBJECT_ACE_TYPE"                Value="0x8" />
                <Set Name="ACCESS_ALLOWED_CALLBACK_ACE_TYPE"            Value="0x9" />
                <Set Name="ACCESS_DENIED_CALLBACK_ACE_TYPE"             Value="0xA" />
                <Set Name="ACCESS_ALLOWED_CALLBACK_OBJECT_ACE_TYPE"     Value="0xB" />
                <Set Name="ACCESS_DENIED_CALLBACK_OBJECT_ACE_TYPE"      Value="0xC" />
                <Set Name="SYSTEM_AUDIT_CALLBACK_ACE_TYPE"              Value="0xD" />
                <Set Name="SYSTEM_ALARM_CALLBACK_ACE_TYPE"              Value="0xE" />
                <Set Name="SYSTEM_AUDIT_CALLBACK_OBJECT_ACE_TYPE"       Value="0xF" />
                <Set Name="SYSTEM_ALARM_CALLBACK_OBJECT_ACE_TYPE"       Value="0x10" />
                <Set Name="SYSTEM_MANDATORY_LABEL_ACE_TYPE"             Value="0x11" />
                <Set Name="SYSTEM_RESOURCE_ATTRIBUTE_ACE_TYPE"          Value="0x12" />
                <Set Name="SYSTEM_SCOPED_POLICY_ID_ACE_TYPE"            Value="0x13" />
            </Enum>
        </Variable>
        <Variable Name="[ACE_TYPE|UCHAR]" Type="Alias" Base="[AceType]">
            <Display Name="UCHAR" />
        </Variable>

        <!-- [AceFlags] -->
        <Variable Name="[AceFlags-BYTE]" Type="Alias" Base="BYTE">
            <Display Name="BYTE" />
            <Flag>
                <Set Name="OBJECT_INHERIT_ACE"                  Value="0x1" />
                <Set Name="CONTAINER_INHERIT_ACE"               Value="0x2" />
                <Set Name="SUB_CONTAINERS_AND_OBJECTS_INHERIT"  Value="0x3" />
                <Set Name="NO_PROPAGATE_INHERIT_ACE"            Value="0x4" />
                <Set Name="INHERIT_ONLY_ACE"                    Value="0x8" />
                <Set Name="INHERITED_ACE"                       Value="0x10" />
            </Flag>
        </Variable>

        <!-- ACE_HEADER -->
        <Variable Name="ACE_HEADER" Type="Struct">
            <Field Type="[AceType]"         Name="AceType" />
            <Field Type="[AceFlags-BYTE]"   Name="AceFlags" />
            <Field Type="WORD"              Name="AceSize" />
        </Variable>

        <!-- ACE -->
        <Variable Name="ACE" Type="Struct">
            <Field Type="ACE_HEADER"    Name="Header" />
            <Field Type="ACCESS_MASK"   Name="AccessMask" />
        </Variable>
        <Variable Name="PACE"  Type="Pointer" Base="ACE" />
        <Variable Name="PACE*" Type="Pointer" Base="PACE" />

        <!-- GENERIC_MAPPING -->
        <Variable Name="GENERIC_MAPPING" Type="Struct">
            <Field Type="ACCESS_MASK" 	Name="GenericRead" />
            <Field Type="ACCESS_MASK" 	Name="GenericWrite" />
            <Field Type="ACCESS_MASK" 	Name="GenericExecute" />
            <Field Type="ACCESS_MASK" 	Name="GenericAll" />
        </Variable>
        <Variable Name="PGENERIC_MAPPING" Type="Pointer" Base="GENERIC_MAPPING" />

        <!-- LUID_AND_ATTRIBUTES -->
        <Variable Name="LUID_AND_ATTRIBUTES" Type="Struct" Pack="4">
            <Field Type="LUID"      Name="Luid" />
            <Field Type="DWORD" 	Name="Attributes" />
        </Variable>
        <Variable Name="PLUID_AND_ATTRIBUTES"                   Type="Pointer"  Base="LUID_AND_ATTRIBUTES" />
        <Variable Name="LUID_AND_ATTRIBUTES [ANYSIZE_ARRAY]"    Type="Array"    Base="LUID_AND_ATTRIBUTES" Count="1" />

        <!-- [PRIVILEGE_SET] -->
        <Variable Name="[PRIVILEGE_SET]" Type="Alias" Base="DWORD">
            <Display Name="DWORD" />
            <Flag>
                <Set Name="PRIVILEGE_SET_ALL_NECESSARY" Value="1" />
            </Flag>
        </Variable>

        <!-- PRIVILEGE_SET -->
        <Variable Name="PRIVILEGE_SET" Type="Struct">
            <Field Type="DWORD"                                 Name="PrivilegeCount" />
            <Field Type="[PRIVILEGE_SET]"                       Name="Control" />
            <Field Type="LUID_AND_ATTRIBUTES [ANYSIZE_ARRAY]"   Name="Privilege" Count="PrivilegeCount" />
        </Variable>
        <Variable Name="PPRIVILEGE_SET" Type="Pointer" Base="PRIVILEGE_SET" />

        <!-- TOKEN_PRIVILEGES -->
        <Variable Name="TOKEN_PRIVILEGES" Type="Struct">
            <Field Type="DWORD"                                 Name="PrivilegeCount" />
            <Field Type="LUID_AND_ATTRIBUTES [ANYSIZE_ARRAY]"   Name="Privileges" />
        </Variable>
        <Variable Name="PTOKEN_PRIVILEGES" Type="Pointer" Base="TOKEN_PRIVILEGES" />

        <!-- ACL -->
        <Variable Name="ACL" Type="Struct">
            <Field Type="BYTE"  	Name="AclRevision" />
            <Field Type="BYTE"  	Name="Sbz1" />
            <Field Type="WORD"   	Name="AclSize" />
            <Field Type="WORD"   	Name="AceCount" />
            <Field Type="WORD"      Name="Sbz2" />
        </Variable>
        <Variable Name="PACL"   Type="Pointer" Base="ACL" />
        <Variable Name="PACL*"  Type="Pointer" Base="PACL" />

        <!-- CREDENTIAL_ATTRIBUTE -->
        <Variable Name="CREDENTIAL_ATTRIBUTE" Type="Struct">
            <Field Type="LPTSTR"  	Name="Keyword" />
            <Field Type="DWORD" 	Name="Flags" />
            <Field Type="DWORD" 	Name="ValueSize" />
            <Field Type="LPBYTE" 	Name="Value" />
        </Variable>
        <Variable Name="PCREDENTIAL_ATTRIBUTE" Type="Pointer" Base="CREDENTIAL_ATTRIBUTE" />

        <!-- CREDENTIAL -->
        <Variable Name="CREDENTIAL" Type="Struct">
            <Field Type="DWORD" 	                Name="Flags" />
            <Field Type="DWORD" 	                Name="Type" />
            <Field Type="LPTSTR" 	                Name="TargetName" />
            <Field Type="LPTSTR" 	                Name="Comment" />
            <Field Type="FILETIME" 	                Name="LastWritten" />
            <Field Type="DWORD" 	                Name="CredentialBlobSize" />
            <Field Type="LPBYTE" 	                Name="CredentialBlob" />
            <Field Type="DWORD" 	                Name="Persist" />
            <Field Type="DWORD" 	                Name="AttributeCount" />
            <Field Type="PCREDENTIAL_ATTRIBUTE" 	Name="Attributes" />
            <Field Type="LPTSTR" 	                Name="TargetAlias" />
            <Field Type="LPTSTR" 	                Name="UserName" />
        </Variable>
        <Variable Name="PCREDENTIAL"    Type="Pointer" Base="CREDENTIAL" />
        <Variable Name="PCREDENTIAL*"   Type="Pointer"  Base="PCREDENTIAL" />
        <Variable Name="PCREDENTIAL**"  Type="Pointer"  Base="PCREDENTIAL*" />

        <!-- CREDENTIAL_TARGET_INFORMATION -->
        <Variable Name="CREDENTIAL_TARGET_INFORMATION" Type="Struct">
            <Field Type="LPTSTR" 	Name="TargetName" />
            <Field Type="LPTSTR" 	Name="NetbiosServerName" />
            <Field Type="LPTSTR" 	Name="DnsServerName" />
            <Field Type="LPTSTR" 	Name="NetbiosDomainName" />
            <Field Type="LPTSTR" 	Name="DnsDomainName" />
            <Field Type="LPTSTR" 	Name="DnsTreeName" />
            <Field Type="LPTSTR" 	Name="PackageName" />
            <Field Type="ULONG" 	Name="Flags" />
            <Field Type="DWORD" 	Name="CredTypeCount" />
            <Field Type="LPDWORD" 	Name="CredTypes" />
        </Variable>
        <Variable Name="PCREDENTIAL_TARGET_INFORMATION"  Type="Pointer" Base="CREDENTIAL_TARGET_INFORMATION" />
        <Variable Name="PCREDENTIAL_TARGET_INFORMATION*" Type="Pointer"  Base="PCREDENTIAL_TARGET_INFORMATION" />

        <!-- POLICY_AUDIT_SID_ARRAY -->
        <Variable Name="POLICY_AUDIT_SID_ARRAY" Type="Struct">
            <Field Type="ULONG" 	Name="UsersCount" />
            <Field Type="PSID*" 	Name="UserSidArray" />
        </Variable>
        <Variable Name="PPOLICY_AUDIT_SID_ARRAY"  Type="Pointer" Base="POLICY_AUDIT_SID_ARRAY" />
        <Variable Name="PPOLICY_AUDIT_SID_ARRAY*" Type="Pointer" Base="PPOLICY_AUDIT_SID_ARRAY" />

        <!-- QUOTA_LIMITS -->
        <Variable Name="QUOTA_LIMITS" Type="Struct">
            <Field Type="SIZE_T" 	        Name="PagedPoolLimit" />
            <Field Type="SIZE_T" 	        Name="NonPagedPoolLimit" />
            <Field Type="SIZE_T" 	        Name="MinimumWorkingSetSize" />
            <Field Type="SIZE_T" 	        Name="MaximumWorkingSetSize" />
            <Field Type="SIZE_T" 	        Name="PagefileLimit" />
            <Field Type="LARGE_INTEGER"     Name="TimeLimit" />
        </Variable>
        <Variable Name="PQUOTA_LIMITS" Type="Pointer" Base="QUOTA_LIMITS" />

        <!-- SecHandle -->
        <Variable Name="SecHandle" Type="Struct">
            <Field Type="ULONG_PTR" 	Name="dwLower" />
            <Field Type="ULONG_PTR"     Name="dwUpper" />
        </Variable>
        
        <!-- PCtxtHandle -->
        <Variable Name="PCtxtHandle" Type="Pointer" Base="SecHandle" />

