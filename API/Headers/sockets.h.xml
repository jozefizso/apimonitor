        <!-- [AI_FLAGS] -->
        <Variable Name="[AI_FLAGS]" Type="Alias" Base="UINT">
            <Display Name="int" />
            <Flag>
                <Set Name="AI_PASSIVE"                      Value="0x00000001" />
                <Set Name="AI_CANONNAME"                    Value="0x00000002" />
                <Set Name="AI_NUMERICHOST"                  Value="0x00000004" />
