        </Variable>

        <!-- struct addrinfo -->
        <Variable Name="struct addrinfo" Type="Struct">
            <Field Type="[AI_FLAGS]"            Name="ai_flags" />
            <Field Type="[AddressFamily]"       Name="ai_family" />
            <Field Type="[SocketType]"          Name="ai_socktype" />
            <Field Type="[SocketProtocol]"      Name="ai_protocol" />
            <Field Type="size_t"                Name="ai_addrlen" />
            <Field Type="char*"                 Name="ai_canonname" />
            <Field Type="struct sockaddr*"      Name="ai_addr" />
            <!-- TODO: Self-Referencing - ai_next is struct addrinfo* -->
            <Field Type="LPVOID"                Name="ai_next" Display="struct addrinfo*" />
        </Variable>
        <Variable Name="struct addrinfo*"       Type="Pointer" Base="struct addrinfo" />
        <Variable Name="const struct addrinfo*" Type="Pointer" Base="struct addrinfo" />
        <Variable Name="struct addrinfo**"      Type="Pointer" Base="struct addrinfo*" />

        <!-- ADDRINFOW -->
        <Variable Name="ADDRINFOW" Type="Struct">
            <Field Type="[AI_FLAGS]"            Name="ai_flags" />
            <Field Type="[AddressFamily]"       Name="ai_family" />
            <Field Type="[SocketType]"          Name="ai_socktype" />
            <Field Type="[SocketProtocol]"      Name="ai_protocol" />
            <Field Type="size_t"                Name="ai_addrlen" />
            <Field Type="PWSTR"                 Name="ai_canonname" />
            <Field Type="struct sockaddr*"      Name="ai_addr" />
            <!-- TODO: Self-Referencing - ai_next is PADDRINFOW -->
            <Field Type="LPVOID"                Name="ai_next" Display="PADDRINFOW" />
        </Variable>
        <Variable Name="const ADDRINFOW*"   Type="Pointer" Base="ADDRINFOW" />
        <Variable Name="PADDRINFOW"         Type="Pointer" Base="ADDRINFOW" />
        <Variable Name="PADDRINFOW*"        Type="Pointer" Base="PADDRINFOW" />

