<ApiMonitor>
    <Include Filename="Headers\windows.h.xml" />
    <Headers>
        <Variable Name="struct sockaddr*"           Type="Alias" Base="LPVOID" />
        <Variable Name="const sockaddr*"            Type="Alias" Base="LPVOID" />
