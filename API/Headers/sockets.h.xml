        <Variable Name="struct hostent*"                         Type="Alias" Base="LPVOID" />
        <Variable Name="socklen_t"                               Type="Alias" Base="int" />

        <Variable Name="struct PROTOENT*"                        Type="Alias" Base="LPVOID" />
