            <Field Type="LPGUID"                Name="ai_provider" />
            <!-- TODO: Self-Referencing - ai_next is PADDRINFOEX -->
            <Field Type="LPVOID"                Name="ai_next" Display="PADDRINFOEX" />
        </Variable>
        <Variable Name="PADDRINFOEX"        Type="Pointer" Base="ADDRINFOEX" />
        <Variable Name="const ADDRINFOEX*"  Type="Pointer" Base="ADDRINFOEX" />
        <Variable Name="PADDRINFOEX*"       Type="Pointer" Base="PADDRINFOEX" />

        <!-- SOCKET -->
        <Variable Name="SOCKET" Type="Alias" Base="UINT_PTR">
            <Enum>
                <Set Name="INVALID_SOCKET" Value="-1" />
            </Enum>
        </Variable>
        <Variable Name="SOCKET [FD_SETSIZE]" Type="Array" Base="SOCKET" Count="64" />

        <!-- fd_set -->
        <Variable Name="fd_set" Type="Struct">
            <Field Type="u_int"                 Name="fd_count" />
