                <Set Name="IPPROTO_DSTOPTS"     Value="60" />
                <Set Name="IPPROTO_ND"          Value="77" />
                <Set Name="IPPROTO_PIM"         Value="103" />
                <Set Name="IPPROTO_PGM"         Value="113" />
                <Set Name="IPPROTO_L2TP"        Value="115" />
