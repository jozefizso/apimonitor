                <Set Name="RESOURCEUSAGE_ALL"                   Value="0x00000013" />
                <Set Name="RESOURCEUSAGE_RESERVED"              Value="0x80000000" />
            </Flag>
        </Variable>

        <!-- [UniversalInfoLevel] -->
        <Variable Name="[UniversalInfoLevel]" Type="Alias" Base="DWORD">
            <Display Name="DWORD" />
            <Enum>
                <Set Name="UNIVERSAL_NAME_INFO_LEVEL"   Value="0x00000001" />
