            <Param Type="int" Name="maxCacheEntries" />
            <Param Type="PRUint32" Name="timeout" />
            <Param Type="PRUint32" Name="ssl3_timeout" />
            <Param Type="const char*" Name="directory" />
            <Return Type="SECStatus" />
        </Api>
